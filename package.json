{
    "name": "n8n-nodes-note-me",
    "version": "0.1.0",
    "description": "n8n community node for NoteMe - automate note creation and reminders. Capture notes from workflows, set date-time reminders, organize with tags, and sync to mobile app.",
    "license": "MIT",
    "homepage": "https://github.com/huynhthanhhuy0605/n8n-nodes-note-me",
    "keywords": [
        "n8n-community-node-package",
        "noteme",
        "note-taking",
        "reminders",
        "automation",
        "productivity",
        "notes",
        "task-management"
    ],
    "author": {
        "name": "huy",
        "email": "huynhthanhhuy0605@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/huynhthanhhuy0605/n8n-nodes-note-me.git"
    },
   "scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/NoteMeApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/NoteMe/NoteMe.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
